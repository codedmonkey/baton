{
    "name": "dirigent/baton",
    "description": "Package manager for Composer registries",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.3"
    },
    "autoload": {
        "psr-4": {
            "Dirigent\\Baton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dirigent\\Baton\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3.0"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
